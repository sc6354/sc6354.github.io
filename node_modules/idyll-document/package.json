{
  "name": "idyll-document",
  "version": "4.0.0-alpha.4",
  "license": "MIT",
  "authors": [
    "Matthew Conlen",
    "Ben Clinkinbeard"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.mjs"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "type": "commonjs",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js -w",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "update-fixtures": "npm run build && node scripts/idl2ast",
    "update-snapshot": " jest --updateSnapshot"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupJest.js"
    ],
    "testMatch": [
      "**/test/**/*.js"
    ],
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "csv-parse/sync": "csv-parse/dist/cjs/sync.cjs",
      "idyll-ast/v1": "idyll-ast/dist/cjs/v1/index.js"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "change-case": "^3.0.1",
    "cross-env": "^5.2.0",
    "csv-parse": "5",
    "falafel": "^2.1.0",
    "fast-deep-equal": "^2.0.1",
    "idyll-ast": "^3.0.0-alpha.1",
    "idyll-compiler": "^5.0.0-alpha.2",
    "idyll-layouts": "^4.0.0-alpha.0",
    "idyll-themes": "^4.0.0-alpha.0",
    "object.entries": "^1.0.4",
    "object.values": "^1.0.4",
    "react-dom-factories": "^1.0.1",
    "react-tooltip": "4.2.15",
    "scrollmonitor": "1.2.3",
    "scrollparent": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-jest": "^27.5.1",
    "concurrently": "^4.1.0",
    "enzyme": "^3.0.0",
    "jest": "27",
    "jest-environment-jsdom": "^27",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^2.70.0",
    "rollup-plugin-jsx": "^1.0.3"
  },
  "peerDependencies": {
    "react": "16 || 17 || 18",
    "react-dom": "16 || 17 || 18"
  },
  "gitHead": "3a22808e5cc5d9a59ac85b6a3be3dbe52613439d"
}
